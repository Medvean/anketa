.noUi-target
	position relative
	top 15px
	margin-left 2px
	width 768px
	cursor pointer
	background none
	border none
	border-radius 0
	box-shadow none

.noUi-handle
	cursor col-resize
	background-color transparent
	background-image url(../images/mark.png)
	background-repeat no-repeat
	background-position 50% 50%
	border none
	border-radius 0
	box-shadow none

	.noUi-horizontal &
		top -12px
		left -8px
		width 17px
		height 14px

.noUi-handle::before,
.noUi-handle::after
	display none